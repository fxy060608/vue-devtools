{
  "name": "@dcloudio/vue-devtools",
  "version": "6.4.5",
  "description": "StandAlone vue-devtools",
  "repository": {
    "url": "https://github.com/vuejs/vue-devtools.git",
    "type": "git",
    "directory": "packages/shell-electron"
  },
  "bin": {
    "vue-devtools": "./bin.js"
  },
  "main": "index.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "start": "node bin.js",
    "dev:client": "webpack --watch",
    "dev:node": "webpack --watch --config webpack.node.config.js",
    "build": "npm run build:app && npm run build:mp && npm run build:web && npm run build:devtools",
    "build:app": "rimraf ./build/app && cross-env NODE_ENV=development webpack  --config webpack.app.config.js",
    "build:mp": "rimraf ./build/mp && cross-env NODE_ENV=development webpack  --config webpack.mp.config.js",
    "build:web": "rimraf ./build/web && cross-env NODE_ENV=development webpack  --config webpack.web.config.js",
    "build:devtools": "rimraf ./dist && cross-env NODE_ENV=development webpack  --config webpack.devtools.config.js"
  },
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "devDependencies": {
    "@dcloudio/types": "^3.0.19",
    "@vue-devtools/app-backend-core": "^0.0.0",
    "@vue-devtools/app-frontend": "^0.0.0",
    "@vue-devtools/build-tools": "^0.0.0",
    "@vue-devtools/shared-utils": "^0.0.0",
    "rimraf": "^3.0.2",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0"
  },
  "files": [
    "build",
    "build-node",
    "icons",
    "types",
    "app.html",
    "app.js",
    "bin.js",
    "index.js",
    "README.md",
    "server.js"
  ]
}